apiVersion: v1
kind: Config
clusters:
  - cluster:
      server: https://kube.omni.haynesops.com/
    name: omni-haynes-edge
contexts:
  - context:
      cluster: omni-haynes-edge
      namespace: default
      user: omni-haynes-edge-admin@haynesnetwork.com
    name: omni-haynes-edge
current-context: omni-haynes-edge
users:
- name: omni-haynes-edge-admin@haynesnetwork.com
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1beta1
      args:
        - oidc-login
        - get-token
        - --oidc-issuer-url=https://omni.haynesops.com/oidc
        - --oidc-client-id=native
        - --oidc-extra-scope=cluster:haynes-edge
      command: kubectl
      env: null
      provideClusterInfo: false
