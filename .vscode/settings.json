{
    "files.associations": {
        "*.json5": "jsonc",
        "./kubernetes/**/*.sops.toml": "plaintext"
    },
    "sops.defaults.ageKeyFile": "age.key",
    "yaml.schemas": {
        "Kubernetes": "./kubernetes/*.yaml",
        "https://gist.githubusercontent.com/thaynes43/21ef53eace2d3c6c8f00c8ee0fe4ec24/raw/b0fd8341723eee62b8a47127d40cb75f00056b67/empty-json-schema.json": "bootstrap/omni/cluster.yaml"
    },
    "vs-kubernetes": {
        "vs-kubernetes.kubeconfig": "./kubeconfig",
        "vs-kubernetes.knownKubeconfigs": [
          "./kubeconfig"
      ]
    },  
    "cSpell.words": [
        "automerge",
        "certificaterequests",
        "datasource",
        "Datasources",
        "devcontainer",
        "fluxcd",
        "gitrepositories",
        "helmreleases",
        "helmrepositories",
        "kubeconform",
        "kustomizations",
        "siderolabs",
        "thaynes"
    ]
}